											<DoubleAnimation Duration="0" To="{Binding Size, ConverterParameter=-0.4566, Converter={StaticResource HeightConverter}, RelativeSource={RelativeSource TemplatedParent}}" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)" Storyboard.TargetName="TitlePanel"/>
										</Storyboard>
									</VisualState>
									<VisualState x:Name="Collapsed1"/>
									<VisualState x:Name="Flipped1">
										<Storyboard>
											<DoubleAnimation Duration="0" To="{Binding Size, ConverterParameter=-1, Converter={StaticResource HeightConverter}, RelativeSource={RelativeSource TemplatedParent}}" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)" Storyboard.TargetName="TitlePanel"/>
											<DoubleAnimation Duration="0" To="180.0" Storyboard.TargetProperty="RotationX" Storyboard.TargetName="ViewportProjection"/>
											<ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="Image">
												<DiscreteObjectKeyFrame KeyTime="0:0:0" Value="Collapsed"/>
											</ObjectAnimationUsingKeyFrames>
											<ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="BackPanel">
												<DiscreteObjectKeyFrame KeyTime="0:0:0" Value="Visible"/>
											</ObjectAnimationUsingKeyFrames>
										</Storyboard>
									</VisualState>
								</VisualStateGroup>
								<VisualStateGroup x:Name="ImageState"/>
							</VisualStateManager.VisualStateGroups>
							<Border.Width>
								<Binding Converter="{StaticResource WidthConverter}" Path="Size" RelativeSource="{RelativeSource TemplatedParent}"/>
							</Border.Width>
							<StackPanel x:Name="Viewport" Height="{Binding Size, Converter={StaticResource HeightConverter}, RelativeSource={RelativeSource TemplatedParent}}" Width="{Binding Size, Converter={StaticResource WidthConverter}, RelativeSource={RelativeSource TemplatedParent}}">
								<StackPanel.Projection>
									<PlaneProjection x:Name="ViewportProjection" CenterOfRotationY="0.25"/>
								</StackPanel.Projection>
								<Grid x:Name="TitlePanel" Height="{Binding Size, ConverterParameter=2, Converter={StaticResource HeightConverter}, RelativeSource={RelativeSource TemplatedParent}}" RenderTransformOrigin="0.5,0.5" Width="{Binding Size, Converter={StaticResource WidthConverter}, RelativeSource={RelativeSource TemplatedParent}}">
									<Grid.RowDefinitions>
										<RowDefinition/>
										<RowDefinition/>
									</Grid.RowDefinitions>
									<Grid.RenderTransform>
										<CompositeTransform/>
									</Grid.RenderTransform>
									<Border Background="{TemplateBinding Background}" Grid.Row="0">
										<TextBlock Foreground="{TemplateBinding Foreground}" FontSize="41" FontFamily="{StaticResource PhoneFontFamilyNormal}" LineStackingStrategy="BlockLineHeight" LineHeight="39" Margin="10,0,0,6" TextWrapping="NoWrap" Text="{TemplateBinding Title}" VerticalAlignment="Bottom"/>
									</Border>
									<Grid x:Name="BackPanel" Background="{TemplateBinding Background}" Height="{Binding Size, Converter={StaticResource HeightConverter}, RelativeSource={RelativeSource TemplatedParent}}" Grid.Row="1" Visibility="Collapsed" Width="{Binding Size, Converter={StaticResource WidthConverter}, RelativeSource={RelativeSource TemplatedParent}}">
										<Grid.Projection>
											<PlaneProjection CenterOfRotationY="0.5" RotationX="180"/>
										</Grid.Projection>
										<Grid.RowDefinitions>
											<RowDefinition Height="*"/>
											<RowDefinition Height="Auto"/>
										</Grid.RowDefinitions>
										<TextBlock x:Name="NotificationBlock" Foreground="{TemplateBinding Foreground}" FontSize="{StaticResource PhoneFontSizeLarge}" FontFamily="{StaticResource PhoneFontFamilyNormal}" LineStackingStrategy="BlockLineHeight" LineHeight="32" Margin="8,8,0,6" Grid.Row="0" TextWrapping="NoWrap" Text="{TemplateBinding Notification}"/>
										<TextBlock x:Name="MessageBlock" Foreground="{TemplateBinding Foreground}" FontSize="{StaticResource PhoneFontSizeNormal}" FontFamily="{StaticResource PhoneFontFamilyNormal}" LineStackingStrategy="BlockLineHeight" LineHeight="23.333" Margin="10,10,10,6" Grid.Row="0" TextWrapping="Wrap" Text="{TemplateBinding Message}"/>
										<TextBlock x:Name="BackTitleBlock" Foreground="{TemplateBinding Foreground}" FontSize="{StaticResource PhoneFontSizeNormal}" FontFamily="{StaticResource PhoneFontFamilySemiBold}" Margin="10,0,0,6" Grid.Row="1" TextWrapping="NoWrap" VerticalAlignment="Bottom"/>
									</Grid>
									<Border x:Name="Image" Background="{TemplateBinding Background}" Grid.Row="1">
										<Image Height="{Binding Size, Converter={StaticResource HeightConverter}, RelativeSource={RelativeSource TemplatedParent}}" Source="{TemplateBinding Source}" Stretch="UniformToFill" Width="{Binding Size, Converter={StaticResource WidthConverter}, RelativeSource={RelativeSource TemplatedParent}}"/>
									</Border>
								</Grid>
							</StackPanel>
						</Border>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
	</phone:PhoneApplicationPage.Resources>

	<phone:PhoneApplicationPage.FontFamily>
		<StaticResource ResourceKey="PhoneFontFamilyNormal"/>
	</phone:PhoneApplicationPage.FontFamily>
	<phone:PhoneApplicationPage.FontSize>
		<StaticResource ResourceKey="PhoneFontSizeNormal"/>
	</phone:PhoneApplicationPage.FontSize>

	<phone:PhoneApplicationPage.DataContext>
		<local:MainPageVM/>
	</phone:PhoneApplicationPage.DataContext>

	<phone:PhoneApplicationPage.ApplicationBar>
    <shell:ApplicationBar x:Name="Application_Bar" IsVisible="True" IsMenuEnabled="True" Mode="Default" Opacity="0.8">
        <shell:ApplicationBar.MenuItems>
            <shell:ApplicationBarMenuItem Text="Know The Devoper" Click="ApplicationBarMenuItem_Click_1"/>
        </shell:ApplicationBar.MenuItems>
        <shell:ApplicationBarIconButton x:Name="help_Btn" IconUri="/Assets/AppBar/appbar.refresh.rest.png" Text="Reload" Click="ApplicationBarIconButton_Click_4"/>
    </shell:ApplicationBar>
</phone:PhoneApplicationPage.ApplicationBar>

	<Grid Margin="0">
		<VisualStateManager.CustomVisualStateManager>
			<ec:ExtendedVisualStateManager/>
		</VisualStateManager.CustomVisualStateManager>
		<VisualStateManager.VisualStateGroups>
			<VisualStateGroup x:Name="VisualStateGroup" ec:ExtendedVisualStateManager.UseFluidLayout="True">
				<VisualStateGroup.Transitions>
					<VisualTransition GeneratedDuration="0:0:0.2">
						<VisualTransition.GeneratedEasingFunction>
							<BackEase EasingMode="EaseOut"/>
						</VisualTransition.GeneratedEasingFunction>
					</VisualTransition>
					<VisualTransition GeneratedDuration="0:0:0.2" To="Hide">
						<VisualTransition.GeneratedEasingFunction>
							<BackEase EasingMode="EaseOut"/>
						</VisualTransition.GeneratedEasingFunction>
					</VisualTransition>
				</VisualStateGroup.Transitions>
				<VisualState x:Name="Hide">
					<Storyboard>
						<ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="grid">
							<DiscreteObjectKeyFrame KeyTime="0">
								<DiscreteObjectKeyFrame.Value>
									<Visibility>Collapsed</Visibility>
								</DiscreteObjectKeyFrame.Value>
							</DiscreteObjectKeyFrame>
						</ObjectAnimationUsingKeyFrames>
					</Storyboard>
				</VisualState>
			</VisualStateGroup>
		</VisualStateManager.VisualStateGroups>

		<Grid x:Name="grid" Canvas.ZIndex="1">
			<toolkit:PerformanceProgressBar IsIndeterminate="{Binding IsVisualBusy, Mode=TwoWay}" Height="20"  Canvas.ZIndex="1" />
			<Image Source="/SplashScreenImage.jpg"/>
		</Grid>

		<controls:Panorama x:Name="Panorama" Title="The Winodws Club" Template="{StaticResource PanoramaControlTemplate1}" >
			<controls:Panorama.Background>
				<ImageBrush ImageSource="/windows---wallpaper.jpg" Opacity="0.58" Stretch="UniformToFill"/>
			</controls:Panorama.Background>
			<controls:Panorama.Foreground>
				<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
					<GradientStop Color="Black" Offset="0"/>
					<GradientStop Color="#FF81858D" Offset="0.33"/>
				</LinearGradientBrush>
			</controls:Panorama.Foreground>

			<!--LayoutRoot is the root grid where all page content is placed-->
 
			<controls:PanoramaItem x:Name="PnRecent" Header="What's New" Style="{StaticResource PanoramaItemStyle1}" Foreground="Black">
				<Grid x:Name="LayoutRoot" Background="Transparent">
					<Grid.RowDefinitions>
						<RowDefinition Height="Auto"/>
						<RowDefinition Height="*"/>
					</Grid.RowDefinitions>

					<Grid VerticalAlignment="Top">
						<Grid.RowDefinitions>
							<RowDefinition Height="Auto"/>
							<RowDefinition Height="Auto"/>
						</Grid.RowDefinitions>
						<toolkit:PerformanceProgressBar IsIndeterminate="{Binding Isbusy, Mode=TwoWay}" Margin="1,1,1,-1"/>
						<TextBlock TextWrapping="Wrap" Text="{Binding Status, Mode=TwoWay}" HorizontalAlignment="Left" Grid.Row="1" FontSize="13.333"/>
					</Grid>
					<ScrollViewer Grid.Row="1" Margin="12,0,12,0">

						<Grid x:Name="ContentPanel">
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="*" />
							</Grid.ColumnDefinitions>
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto" />
								<RowDefinition Height="Auto" />
								<RowDefinition Height="Auto" />
								<RowDefinition Height="Auto" />
								<RowDefinition Height="Auto" />
								<RowDefinition Height="Auto" />
								<RowDefinition Height="100" />
							</Grid.RowDefinitions>
							<toolkit:HubTile x:Name="first" Width="200" Height="200" Margin="4,10" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Message="{Binding FirstTile.PlainSummary, Mode=TwoWay}" Notification="{Binding FirstTile.Title, Mode=TwoWay}" Source="{Binding FirstTile.Image, Mode=TwoWay}" Title="{Binding FirstTile.Title, Mode=TwoWay}" Size="Medium" Background="{StaticResource PhoneAccentBrush}" Style="{StaticResource HubTileStyle2}" />
							<toolkit:HubTile x:Name="Second_" Width="200" Height="200" Grid.Column="1" Margin="4,10" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Message="{Binding SeconTile.PlainSummary, Mode=TwoWay}" Notification="{Binding SeconTile.Title, Mode=TwoWay}" Source="{Binding SeconTile.Image, Mode=TwoWay}" Title="{Binding SeconTile.Title, Mode=TwoWay}" Size="Medium" />
							<toolkit:HubTile x:Name="Third" Width="200" Height="200" Grid.Row="1" Margin="4,10" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Message="{Binding ThirdTile.PlainSummary, Mode=TwoWay}" Notification="{Binding ThirdTile.Title, Mode=TwoWay}" Source="{Binding ThirdTile.Image, Mode=TwoWay}" Title="{Binding ThirdTile.Title, Mode=TwoWay}" Size="Medium" />
							<toolkit:HubTile x:Name="Fourth" Width="200" Height="200" Grid.Row="1" Grid.Column="1" Margin="4,10" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Message="{Binding ForthTile.PlainSummary, Mode=TwoWay}" Notification="{Binding ForthTile.Title, Mode=TwoWay}" Source="{Binding ForthTile.Image, Mode=TwoWay}" Title="{Binding ForthTile.Title, Mode=TwoWay}" Size="Medium" />
							<toolkit:HubTile x:Name="Fifth_" Width="200" Height="200" Grid.Row="2" Margin="4,10" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Message="{Binding FifthTile.PlainSummary, Mode=TwoWay}" Notification="{Binding FifthTile.Title, Mode=TwoWay}" Source="{Binding FifthTile.Image, Mode=TwoWay}" Title="{Binding FifthTile.Title, Mode=TwoWay}" Size="Medium"/>
							<toolkit:HubTile x:Name="Sixth" Width="200" Height="200" Grid.Row="2" Grid.Column="1" Margin="4,10" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Message="{Binding SixthTile.PlainSummary, Mode=TwoWay}" Notification="{Binding SixthTile.Title, Mode=TwoWay}" Source="{Binding SixthTile.Image, Mode=TwoWay}" Title="{Binding SixthTile.Title, Mode=TwoWay}" Size="Medium"/>
							<toolkit:HubTile x:Name="Seventh" Width="200" Height="200" Grid.Row="3" Margin="4,10" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Message="{Binding SeventhTile.PlainSummary, Mode=TwoWay}" Notification="{Binding SeventhTile.Title, Mode=TwoWay}" Source="{Binding SeventhTile.Image, Mode=TwoWay}" Title="{Binding SeventhTile.Title, Mode=TwoWay}" Size="Medium"/>
							<toolkit:HubTile x:Name="Eigtth" Width="200" Height="200" Grid.Row="3" Grid.Column="1" Margin="4,10" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Message="{Binding EightthTile.PlainSummary, Mode=TwoWay}" Notification="{Binding EightthTile.Title, Mode=TwoWay}" Source="{Binding EightthTile.Image, Mode=TwoWay}" Title="{Binding EightthTile.Title, Mode=TwoWay}" Size="Medium"/>
							<toolkit:HubTile x:Name="Ninth" Width="200" Height="200" Grid.Row="4" Margin="4,10" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Message="{Binding NinthTile.PlainSummary, Mode=TwoWay}" Notification="{Binding NinthTile.Title, Mode=TwoWay}" Source="{Binding NinthTile.Image, Mode=TwoWay}" Title="{Binding NinthTile.Title, Mode=TwoWay}" Size="Medium"/>
							<toolkit:HubTile x:Name="Tenth" Width="200" Height="200" Grid.Row="4" Grid.Column="1" Margin="4,10" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Message="{Binding TenthModel.PlainSummary, Mode=TwoWay}" Notification="{Binding TenthModel.Title, Mode=TwoWay}" Source="{Binding TenthModel.Image, Mode=TwoWay}" Title="{Binding TenthModel.Title, Mode=TwoWay}" Size="Medium"/>
					

						</Grid>
					
					</ScrollViewer>

					<!--TitlePanel contains the name of the application and page title-->
					
				</Grid>
			</controls:PanoramaItem>
			<controls:PanoramaItem Style="{StaticResource PanoramaItemStyle1}" x:Name="PnWindows" Header="Windows" Foreground="Black">
				<Border x:Name="WContainer"/>
			</controls:PanoramaItem>
			<controls:PanoramaItem Style="{StaticResource PanoramaItemStyle1}" x:Name="PnSecurity" Header="Security" Foreground="Black">
				<Border x:Name="SecContent"/>
			</controls:PanoramaItem>
			<controls:PanoramaItem Style="{StaticResource PanoramaItemStyle1}" x:Name="Pndownloads" Header="Downloads" Foreground="Black">
				<Border x:Name="Downlcontainer"/>
			</controls:PanoramaItem>
			<controls:PanoramaItem Style="{StaticResource PanoramaItemStyle1}" x:Name="PnIE" Header="I.E" Foreground="Black">
				<Border x:Name="IEContainer"/>
			</controls:PanoramaItem>
			<controls:PanoramaItem x:Name="AboutPanorama" Header="About" Style="{StaticResource PanoramaItemStyle1}" Foreground="Black">
				<Border x:Name="AboutCOntainer"/>
			</controls:PanoramaItem>
		</controls:Panorama>
 
	</Grid>
 
</phone:PhoneApplicationPage>